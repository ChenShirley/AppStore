		var age = $("input[name='survey[age]']").val();
		var gender = $("input[name='survey[gender]']:checked").val();
		var income = $("input[name='survey[income]']:checked").val();

		if(why.length == 0){
			$("#why_error").text("This field is required.");
		} else {
			$("#why_error").text("");
		}
		if(age.length == 0){
			$("#age_error").text("This field is required.");
		} else {
			$("#age_error").text("");
		}

		if(involvement1===undefined){
			$("#involvement1_error").text("This field is required.");
		} else {
			$("#involvement1_error").text("");
		}
		if(involvement2===undefined){
			$("#involvement2_error").text("This field is required.");
		} else {
			$("#involvement2_error").text("");
		}
		if(involvement3===undefined){
