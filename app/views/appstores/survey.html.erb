		}
		if(rf1===undefined){
			$("#rf1_error").text("This field is required.");
		} else {
			$("#rf1_error").text("");
		}
		if(rf2===undefined){
			$("#rf2_error").text("This field is required.");
		} else {
			$("#rf2_error").text("");
		}
		if(rf3===undefined){
			$("#rf3_error").text("This field is required.");
		} else {
			$("#rf3_error").text("");
		}
		if(rf4===undefined){
			$("#rf4_error").text("This field is required.");
		} else {
			$("#rf4_error").text("");
		}
		if(rf5===undefined){
			$("#rf5_error").text("This field is required.");
